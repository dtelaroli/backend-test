{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest",
    "dev": "NODE_ENV=development nodemon --inspect -r dotenv-flow/config app.js",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js"
    ],
    "testTimeout": 30000,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFiles": [
      "dotenv-flow/config"
    ],
    "setupFilesAfterEnv": [
      "./db/config/prepare.js"
    ],
    "silent": true
  },
  "author": "Denilson Telaroli",
  "license": "ISC",
  "devDependencies": {
    "apollo-server-testing": "^2.17.0",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "sqlite3": "^5.0.0"
  },
  "dependencies": {
    "apollo-server": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "apollo-server-plugin-response-cache": "^0.5.3",
    "await-to-js": "^2.1.1",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql-scalars": "^1.2.7",
    "lodash": "^4.17.20",
    "pg": "^8.3.3",
    "pino": "^6.6.1"
  }
}
